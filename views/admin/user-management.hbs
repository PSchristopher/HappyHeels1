<body class="fixed-left">


    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-body">

                    <h4 class="mt-0 header-title"style="text-align:center ;">List Of ALL User</h4>
                    <p class="text-muted mb-4 font-13">The Buttons extension for DataTables
                        provides a common set of options, API methods and styling to display
                        buttons on a page that will interact with a DataTable. The core library
                        provides the based framework upon which plug-ins can built.
                    </p>

                    <table id="datatable-buttons" class="table table-striped table-bordered dt-responsive nowrap"
                        style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                        <thead>
                            <tr>
                                <th>S/no</th>
                                <th>Name</th>
                                <th>Email</th>
                                <th>Phone</th>
                                <th>Status</th>
                                <th>Action</th>
                            </tr>
                        </thead>


                        <tbody>
                            {{#each usersData}}
                            <tr>
                                <td style="text-align:center ;">{{ @index}}</td>
                                <td style="text-align:center ;">{{this.name}}</td>
                                <td style="text-align:center ;">{{this.email}}</td>
                                <td style="text-align:center ;">{{this.phone}}</td>
                                <td style="text-align:center ;">{{this.state}}</td>
                                <td style="text-align:center ;">
                                    {{#if this.state}}

                                    {{!-- <button type="button" class="btn btn-outline-info"><i
                                            class="fa-solid fa-user-lock"></i> Block</button> --}}
                                    <a href="/admin/usermanagement/{{this._id}}"> <button type="button"
                                            class="btn btn-outline-info btn-round waves-effect waves-light"><i
                                                class="mdi mdi-send mr-2"></i>Block</button></a>
                                    {{else}}
                                    {{!-- <button type="button" class="btn btn-outline-danger"><i
                                            class="fa-solid fa-user-lock"></i> UnBlock</button></a> --}}
                                    <a href="/admin/usermanagements/{{this._id}}"><button type="button"
                                            class="btn btn-outline-danger btn-round waves-effect waves-light"><i
                                                class="mdi mdi-power mr-2"></i>Unblock</button></a>
                                    {{/if}}
                                </td>
                            </tr>
                            {{/each}}
                        </tbody>
                    </table>

                </div>
            </div>
        </div> <!-- end col -->
    </div> <!-- end row -->

    </div><!-- container -->

    </div> <!-- Page content Wrapper -->

    </div> <!-- content -->



    </div>
    <!-- End Right content here -->

    </div>
    <!-- END wrapper -->


</body>