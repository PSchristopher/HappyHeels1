    {{!-- lkjhgfjklkjh --}}

    <section class="container">
        {{#each orders}}
        <div class="row">
            <div class="col-md-4 pt-4">
                <p>Order id : {{this._id}}</p>

            </div>
            <div class="col-md-4 pt-4 text-center">
                <h6>Order Date:{{this.date}}</h6>
            </div>

            <div class="col-md-4 pt-4">
                <div class="ml-auto" style="width:fit-content">
                    <button>Track  Order</button>
                </div>
            </div>

        </div>
        <hr style="border-bottom: 1px solid #1a1a1a;;">
        <div class="row">
            <div class="col-md-3 pt-4">

                <img style="width: 80px;"src="/imageUpload/{{this.product.image.[0]}}" alt="some">
            </div>
            <div class="col-md-3 pt-4 text-start">
                <h6>{{this.product.name}}</h6>
                <p>₹ {{this.product.price}}</p>
            </div>

            <div class="col-md-3 pt-4 text-center">
                <p>{{this.status}}</p>
            </div>
            <div class="col-md-3 pt-4 text-center">
                <h6>Delivery Address</h6>
                <p>{{this.deliverDetails.firstName}},{{this.deliverDetails.phone}}</p>
                <p>{{this.deliverDetails.streetAddress}}</p>
                <p>{{this.deliverDetails.city}}</p>
                <p>{{this.deliverDetails.zip}}</p>
                <p>{{this.deliverDetails.country}}</p>

            </div>
        </div>
        <hr style="border-bottom: 1px solid #1a1a1a;;">
        <div class="row">
            <div class="col-md-4 pt-4">
                <p>Total Amount : {{this.totalAmount}}</p>
            </div>
            <div class="col-md-4 pt-4 text-center">
                <h6>payment-method{{this.paymentMethod}}</h6>
            </div>
            <div class="col-md-4 pt-4">
                <div class="ml-auto" style="width:fit-content">
                    <button>cancel</button>
                </div>
            </div>
        </div>
        <hr style="border-bottom: 1px solid #1a1a1a;;">
        {{/each}}
    </section>
    <hr style="border-bottom: 1px solid #1a1a1a;;">